{
  "name": "MacOsAppShortcut",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint ./src",
    "start": "react-native start",
    "test": "jest --coverage",
    "pod": "cd ios && pod install",
    "android:apk": "cd android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release",
    "android:aab": "cd android && ./gradlew bundleRelease && open ./app/build/outputs/bundle/release",
    "android:fastlane:beta": "cd android && fastlane deploy_internal",
    "android:fastlane:production": "cd android && fastlane deploy_production"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/firestore": "^18.7.3",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.6.3",
    "prettier": "^3.1.1",
    "react-native-svg-transformer": "^1.2.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
